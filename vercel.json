{
    "version": 2,
    "name": "SearchSage",
    "builds": [
        { "src": "app.py", "use": "@vercel/python" }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "/app.py" }
    ],
    "functions": {
        "app.py": {
            "maxDuration": 10,
            "memory": 128
        }
    }
}
